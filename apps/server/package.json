{
  "name": "server",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "build": "nitro build",
    "build:tsdown": "tsdown",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "nitro dev",
    "dev:simple": "bun run --hot src/index.ts",
    "start": "bun run dist/index.js"
  },
  "dependencies": {
    "@babypeek/api": "workspace:*",
    "@babypeek/db": "workspace:*",
    "dotenv": "catalog:",
    "hono": "catalog:",
    "nitro": "^3.0.1-alpha.1",
    "rollup": "^4.54.0",
    "workflow": "^4.0.1-beta.35",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@babypeek/config": "workspace:*",
    "@types/bun": "catalog:",
    "tsdown": "^0.16.5",
    "typescript": "catalog:"
  }
}
